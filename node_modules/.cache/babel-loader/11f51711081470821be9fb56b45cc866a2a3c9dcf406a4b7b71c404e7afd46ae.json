{"ast":null,"code":"export default {\n  props: ['currentSection'],\n  data() {\n    return {\n      sidebarVisible: false,\n      links: [{\n        name: 'Home',\n        href: '#Homesection'\n      }, {\n        name: 'About',\n        href: '#Aboutsection'\n      }, {\n        name: 'Portfolio',\n        href: '#Portfoliosection'\n      }, {\n        name: 'Skills',\n        href: '#SkillsSection'\n      }, {\n        name: 'Contact',\n        href: '#Conract'\n      }]\n    };\n  },\n  methods: {\n    toggleSidebar() {\n      this.sidebarVisible = !this.sidebarVisible;\n    },\n    closeSidebar() {\n      this.sidebarVisible = false;\n    }\n  }\n};","map":{"version":3,"names":["props","data","sidebarVisible","links","name","href","methods","toggleSidebar","closeSidebar"],"sources":["F:\\MyPortfolio\\src\\components\\HeaderComponent.vue"],"sourcesContent":["<template>\r\n  <header class=\"sticky-top\">\r\n    <nav class=\"navbar navbar-expand-lg bg-dark\">\r\n      <div class=\"container\">\r\n        <a class=\"navbar-brand text-light fs-2\" href=\"#\">\r\n          <strong class=\"text-danger\">Jehadov</strong>\r\n        </a>\r\n        <button class=\"navbar-toggler text-light bg-danger\" type=\"button\" @click=\"toggleSidebar\">\r\n          <span class=\"navbar-toggler-icon text-light\"></span>\r\n        </button>\r\n        <div :class=\"['custom-sidebar', { 'show': sidebarVisible }]\" id=\"navbarNav\">\r\n          <ul class=\"navbar-nav text-center\">\r\n            <li class=\"nav-item\" v-for=\"link in links\" :key=\"link.name\">\r\n              <a\r\n                class=\"nav-link custom-underline ms-2 red\"\r\n                :href=\"link.href\"\r\n                @click=\"closeSidebar\"\r\n                :class=\"{ active: currentSection === link.name }\"\r\n              >\r\n                {{ link.name }}\r\n              </a>\r\n            </li>\r\n          </ul>\r\n        </div>\r\n        <div class=\"backdrop\" v-if=\"sidebarVisible\